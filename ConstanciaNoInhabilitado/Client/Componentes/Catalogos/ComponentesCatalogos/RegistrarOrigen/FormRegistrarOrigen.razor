@using ConstanciaNoInhabilitado.Shared.Entities.Login;


<MudItem xs="5">
    <MudTextField @bind-Value="RegistrarOrigenInhabilitacion.Descripcion" Label="Introduzca la descripcion del origen" Variant="Variant.Outlined" />
</MudItem>


<MudItem xs="3">
    <MudButton Variant="Variant.Filled" Color="Color.Secondary" Style="width:200px;" OnClick="EventCall">Registrar Origen de Inhabilitación </MudButton>
</MudItem>

@code {
    [Parameter]
    public OrigenesInhabilitacion RegistrarOrigenInhabilitacion { get; set; } = new();

    [Parameter]
    public EventCallback eventCallback { get; set; }

    public async Task EventCall ()
    {
        await eventCallback.InvokeAsync(RegistrarOrigenInhabilitacion);
    }
}
